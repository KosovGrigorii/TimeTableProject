@model IEnumerable<string>

<div>
    <input list="filtered" type="text" placeholder="Name"/>
    <datalist id="filtered">
        @foreach (var item in Model)
        {
            <option value="@item"></option>
        }
    </datalist>
</div>
<div>
    <input type="number" step="1" placeholder="Hours amount"/>
</div>
